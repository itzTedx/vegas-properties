    "embla-carousel-react": "^8.6.0",
    "embla-carousel-wheel-gestures": "^8.1.0",
    "graphql": "^16.8.1",
    "gsap": "^3.13.0",
    "lucide-react": "^0.542.0",
    "motion": "^12.23.12",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
